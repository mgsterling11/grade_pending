<p id="notice"><%= notice %></p>

<strong>You had mulitple results...</strong>
<ul>
    
    <% @restaurants_array.each do |restaurant| %>
    <p>
      <br>
      <li>
      <p>
        <strong>Name:</strong>
        <%= restaurant.name %>
      </p>

      <p>
        <strong>Grade:</strong>
        <%= restaurant.grade %>
      </p>

      <p>
        <strong>Inspection_date:</strong>
        <%= restaurant.inspection_date %>
      </p>

      <p>
        <strong>Cuisine:</strong>
        <%= restaurant.cuisine_type.cuisine_description %>
      </p>

      <p>
        <strong>Violation(s):</strong>
        <%= restaurant.violation_description.violation_description %>
      </p>

      <p>
        <strong>Violation Code(s):</strong>
        <%= restaurant.violation_description.violation_code %>
      </p>
      
      <p>
        <strong>Address:</strong>
        <%= restaurant.address.building %>
        <%= restaurant.address.street %>
        <%= restaurant.address.boro %>
        <%= restaurant.address.zipcode %>
      </p>
      
      <p>
        <strong>Phone:</strong>
        <%= restaurant.address.phone %>
      </p>
    </li>
    </p>
    
    
    <%end%>
  </ul>


    